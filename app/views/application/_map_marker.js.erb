
<%#
  This snippet assumes the calling block is Javascript and has already
  declared a Javascript variable called 'map' and one called 'markers'.
%>

markers.push(
  new google.maps.Marker({
    position: {
      lat: <%= device.latitude %>,
      lng: <%= device.longitude %>
    },
    icon: {
      url: "<%= device_map_icon_url(device) %>"
    },
    map: map
  })
)
